<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">


    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="platformTransactionManager">
        <constructor-arg value="org.springframework.transaction.PlatformTransactionManager"/>
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="nodeDao">
        <constructor-arg value="org.opennms.netmgt.dao.api.NodeDao"/>
    </bean>

    <bean id="nodeTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.NodeTopologyServiceImpl">
      <property name="nodeDao" ref="nodeDao" />
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="bridgeElementDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.BridgeElementDao"/>
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="bridgeBridgeLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.BridgeBridgeLinkDao"/>
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="bridgeMacLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.BridgeMacLinkDao"/>
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="bridgeStpLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.BridgeStpLinkDao"/>
    </bean>

    <bean id="bridgeTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.BridgeTopologyServiceImpl">
        <property name="bridgeElementDao" ref="bridgeElementDao" />
        <property name="bridgeBridgeLinkDao" ref="bridgeBridgeLinkDao" />
        <property name="bridgeMacLinkDao" ref="bridgeMacLinkDao" />
        <property name="bridgeStpLinkDao" ref="bridgeStpLinkDao" />
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="ipNetToMediaDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.IpNetToMediaDao"/>
    </bean>

    <bean id="ipNetToMediaTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.IpNetToMediaTopologyServiceImpl">
        <property name="ipNetToMediaDao" ref="ipNetToMediaDao" />
    </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="cdpElementDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.CdpElementDao"/>
    </bean>
    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="cdpLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.CdpLinkDao"/>
    </bean>
        
   <bean id="cdpTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.CdpTopologyServiceImpl">
      <property name="cdpLinkDao" ref="cdpLinkDao" />
      <property name="cdpElementDao" ref="cdpElementDao" />
   </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="isisElementDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.IsIsElementDao"/>
    </bean>
    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="isisLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.IsIsLinkDao"/>
    </bean>

   <bean id="isisTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.IsisTopologyServiceImpl">
      <property name="isisLinkDao" ref="isisLinkDao" />
      <property name="isisElementDao" ref="isisElementDao" />
   </bean>

    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="lldpElementDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.LldpElementDao"/>
    </bean>
    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="lldpLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.LldpLinkDao"/>
    </bean>

   <bean id="lldpTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.LldpTopologyServiceImpl">
      <property name="lldpLinkDao" ref="lldpLinkDao" />
      <property name="lldpElementDao" ref="lldpElementDao" />
   </bean>
   
       <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="ospfElementDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.OspfElementDao"/>
    </bean>
    <bean class="org.easymock.EasyMock" factory-method="createNiceMock" primary="true" id="ospfLinkDao">
        <constructor-arg value="org.opennms.netmgt.enlinkd.persistence.api.OspfLinkDao"/>
    </bean>
   
   <bean id="ospfTopologyService" class="org.opennms.netmgt.enlinkd.service.impl.OspfTopologyServiceImpl">
      <property name="ospfLinkDao" ref="ospfLinkDao" />
      <property name="ospfElementDao" ref="ospfElementDao" />
   </bean>


</beans>

